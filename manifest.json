{
  "manifest_version": 2,
  "name": "Bus#115",
  "version": "0.0.1",
  "icons": {
    "48": "image/icon-48.png"
  },
  "options_ui": {
    "page": "option/index.html",
    "open_in_tab": true
  },

  "background": {
    "page": "background/main.html"
  },

  "content_scripts": [{
    "all_frames": false,
    "matches": [
      "*://115.com/*",
      "*://*.115.com/*"
    ],
    "js": [
      "content/bridge.js"
    ]
  }],

  "web_accessible_resources": [
    "web/inject.js"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "<all_urls>",
    "downloads",
    "storage",
    "tabs"
  ]
}